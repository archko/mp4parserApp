                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="archko.test"/>
            </intent-filter>
        </activity>

        <activity android:name="org.bestforce.testmp4parser.MainActivity" android:label="video/bestforce_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
